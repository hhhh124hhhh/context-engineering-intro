# API Configuration
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000

# Environment
VITE_NODE_ENV=development

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false

# Game Configuration
VITE_GAME_TIMEOUT=30000
VITE_MAX_DECK_SIZE=30
VITE_STARTING_HAND_SIZE=5

# Animation Settings
VITE_ENABLE_ANIMATIONS=true
VITE_ANIMATION_SPEED=1.0

# Sound Settings
VITE_ENABLE_SOUNDS=true
VITE_VOLUME=0.7